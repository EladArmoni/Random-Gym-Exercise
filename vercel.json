{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/exercise/(.*)",
      "dest": "/api/exercise/$1"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "/api/user/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
